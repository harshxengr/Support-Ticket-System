generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Ticket {
  id          Int      @id @default(autoincrement())
  title       String   @db.VarChar(200)
  description String
  category    String
  priority    String
  status      String   @default("open")
  createdAt   DateTime @default(now())
}
